@incollection{huenemann2013nietzsches_illness,
    author = {Huenemann, Charles},
    isbn = {9780199534647},
    title = "{Nietzsche’s Illness}",
    booktitle = "{The Oxford Handbook of Nietzsche}",
    publisher = {Oxford University Press},
    year = {2013},
    month = {09},
    abstract = "{This article examines how Nietzsche’s illness bears on his philosophical ideas. It demonstrates that the long-standard explanation for Nietzsche’s dementia—syphilis—is almost certainly false. The cause is much more likely to have been a brain tumor, which had caused him severe headaches and eye problems since childhood. Nietzsche also suffered from a host of digestive problems. It is no wonder that he puts such great weight on “health” and especially the kind of health that overcomes sickness and suffering. When Nietzsche values “madness,” it is a healthy and philosophical madness exemplified in Zarathustra and which Nietzsche tried to cultivate in himself.}",
    doi = {10.1093/oxfordhb/9780199534647.013.0004},
    url = {https://doi.org/10.1093/oxfordhb/9780199534647.013.0004},
